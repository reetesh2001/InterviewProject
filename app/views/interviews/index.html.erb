<table>
  <thead>
    <tr>
      <th>Candidate</th>
      <th>Employee</th>
      <th>Round</th>
      <th>Start_Interview_time</th>
      <th>End_Interview_time</th>
      <th>Status</th>
      <th>Feedback</th>
      <th colspan="2">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @interviews.each do |interview| %>
      <tr>
        <td><%= Candidate.find(interview[:candidate_id]).name %></td>
        <td><%= User.find(interview[:employee_id]).name %></td>
        <td><%= interview.round %></td>
        <td><%=interview.interview_time%></td>
        <td><%=interview.end_interview_time%></td>
        <td><%= interview.status %></td>
        <td><%= interview.feedback %></td>
        <td>
          <%= button_to 'Delete', user_interview_path(@user, interview), method: :delete ,class: "delete"%>
        </td>
        <td><%= link_to 'Edit', edit_user_interview_path(@user, interview),class: "edit" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
